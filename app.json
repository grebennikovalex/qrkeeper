{
  "expo": {
    "name": "QRWallet",
    "slug": "qrwallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconWallet.png",
    "splash": {
      "image": "./assets/splashWallet.png",
      "resizeMode": "cover",
      "backgroundColor": "#915eba"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconWallet.png",
        "backgroundColor": "#915eba"
      }
    }
  }
}
