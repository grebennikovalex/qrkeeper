{
  "expo": {
    "name": "QRWallet",
    "slug": "qrwallet",
    "version": "0.2.3",
    "orientation": "portrait",
    "icon": "./assets/app_icon_combined.png",
    "splash": {
      "image": "./assets/qrwallet_splash_lng.png",
      "resizeMode": "cover",
      "backgroundColor": "#5D5FEF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "hidden": true
    },
    "android": {
      "versionCode": 1,
      "permissions": ["CAMERA"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon_img.png",
        "backgroundImage": "./assets/app_icon_bg.png",
        "backgroundColor": "#5D5FEF"
      },
      "package": "com.grebennikovalex.qrwallet"
    },
    "description": ""
  }
}
